[Script]
isInvitedColor = "575757"
inviteColor = "F6F7F9"
isSucceedColor = "D7F9FF"
isFinishedColor = "C1C1C1"
errorColor1 = "FFFFFF"
errorColor2 = "E9EBEE"

screenWidth = Plugin.Sys.GetScRX()
screenHeight = Plugin.Sys.GetScRY()
 
If screenWidth = 1920 and screenHeight = 1080 Then  
	isInvitedXLeft = 1110
	isInvitedYUp = 273
	isInvitedXRight = 1133
	isInvitedYDown = 335
	
	InvitedXLeft = 1123
	InvitedYUp = 273
	InvitedXRight = 1161
	InvitedYDown = 335
	
	isFinishedX = 1183
	isFinishedY = 680
	
	ClickBorderRight = 1161
	ClickBorderDown = 698
	
	ElseIf screenWidth = 1280 and screenHeight = 1024 Then
	isInvitedXLeft = 790
	isInvitedYUp = 267
	isInvitedXRight = 813
	isInvitedYDown = 329
	
	InvitedXLeft = 803
	InvitedYUp = 267
	InvitedXRight = 841
	InvitedYDown = 329
	
	isFinishedX = 862
	isFinishedY = 674
	
	ClickBorderRight = 842
	ClickBorderDown = 692
		
	 
End If

YSpace = 61
Times = 1

MaxTimes = 7
isInvited = False

MsgBox ("准备开始自动邀请！"+Chr(13)+"切换到FB后开始！")



EndColor = GetPixelColor(isFinishedX,isFinishedY)
While EndColor <> isFinishedColor
	EndColor = GetPixelColor(isFinishedX, isFinishedY)
   
   	For sarchTimes = 0 To ( MaxTimes - 1 )
    	//MsgBox ("进入For循环"+Chr(13)+"EndColor="+EndColor+Chr(13)+"sarchTimes="+CStr(sarchTimes)+Chr(13)+"isInvited="+CStr(isInvited))
    	
    	FindColor isInvitedXLeft, (isInvitedYUp + sarchTimes * YSpace), isInvitedXRight, (isInvitedYDown + sarchTimes * YSpace), isInvitedColor, intX1, intY1
    	//FindColor isInvitedXLeft, (isInvitedYUp + sarchTimes * YSpace), isInvitedXRight, (isInvitedYDown + sarchTimes * YSpace), isSucceedColor, intX2, intY2
		
    	If intX1 > 0 or intY1 > 0 Then  
      		//MsgBox("sarchTimes="+CStr(sarchTimes)+Chr(13)+"检测到已邀请"+Chr(13)+CStr(intX1)+Chr(13)+CStr(intY1)+Chr(13)+CStr(intX2)+Chr(13)+CStr(intY2))
      		isInvited = True
      	Else 
      		//MsgBox("sarchTimes="+CStr(sarchTimes)+Chr(13)+"可邀请")
      		isInvited = False
		End If	
		
		If (isInvited = False) Then  	
    	  	FindColorEx InvitedXLeft, (InvitedYUp + sarchTimes * YSpace), InvitedXRight, (InvitedYDown + sarchTimes * YSpace), inviteColor, 0, 0.85, intX, intY
			//MsgBox("XLeft="+CStr(InvitedXLeft)+Chr(13)+"YUp="+CStr(InvitedYUp + sarchTimes * YSpace)+Chr(13)+"XRight="+CStr(InvitedXRight)+Chr(13)+"YDown="+CStr(InvitedYDown + sarchTimes * YSpace)+Chr(13)+"intX="+CStr(intX)+Chr(13)+"intY="+CStr(intY))
			
			If intX > 0 or intY > 0 Then  
   	 		  	//MsgBox("准备开始点击")
   	 		  	Rem Click
   	 		  	
   	 		  	tryClick = 0
   	 		  	Do While true
    			ClickX = intX + Int((35 - 0 + 1) * Rnd + 0)
    			ClickY = intY + Int((10 - 0 + 1) * Rnd + 0)
    			tryClick = tryClick + 1
    			clickColor = GetPixelColor( ClickX, ClickY )
				If ClickX < ClickBorderRight and ClickY < ClickBorderDown and clickColor <> errorColor1 and clickColor <> errorColor2 Then    
				Exit Do
				End If
				
				If tryClick > 50 Then    
				MsgBox ("o法_定c粑恢茫手狱c艉罄^m。")
				End If
				
				Loop
   	 		  	
   	 		  	MoveTo ClickX, ClickY
   	 		  	Delay Int((1500 - 500 + 1) * Rnd + 500)
   	 		  	LeftDown 1
   	 		  	Delay Int((500 - 300 + 1) * Rnd + 300)
   	 		  	LeftUp 1
   	 		  	
				Delay Int((3000 - 1500 + 1) * Rnd + 1500)
				
			End If
		End If    
	Next
   
   MouseWheel - 2 
   
   sarchTimes = 0
     
   Delay Int((2500 - 1500 + 1) * Rnd + 1500)
   Times = Times + 1
   
	If Times = 11 Then  
      Delay Int((15000 - 13000 + 1) * Rnd + 1500)
      Times = 1
	End If
	
Wend

MsgBox ("已完成全部邀！")
EndScript
